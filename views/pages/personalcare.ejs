<%- include ("./partials/header.ejs") %>
<body>
    <div class="row">
        <div class="col-sm-3">
            <div class="left-sidebar">
                <div class="price-range">
                    <h2>Price Range</h2>
                    <div class="well">
                        <div class="slider slider-horizontal" style="width: 182px;">
                            <div class="slider-track">
                                <div class="slider-selection" style="left: 70.8333%; width: 18.3333%;"></div>
                                <div class="slider-handle round left-round" style="left: 70.8333%;"></div>
                                <div class="slider-handle round" style="left: 89.1667%;"></div>
                            </div>
                            <div class="tooltip top" style="top: -30px; left: 113.1px;">
                                <div class="tooltip-arrow"></div>
                                <div class="tooltip-inner">425 : 535</div>
                            </div><input type="text" class="span2" value="" data-slider-min="0"
                                data-slider-max="600" data-slider-step="5" data-slider-value="[250,450]"
                                id="sl2" >
                        </div><br>
                        <b>$ 0</b> <b class="pull-right">$ 600</b>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="wrapper">
        <div class="container2">
            <div class="title">Personal Care</div>

            <div class="filter-items" >
                <input type="checkbox" id="collapse" name="collapse">
                <label for="collapse" class="label-collapse">Brand</label>
                    <div class="brands" id="brands">
                        <hr>
                        <input type="checkbox" id="b1" name="brand1" value="GSK">
                        <label for="b1">GSK</label>
                        <br>
                        <input type="checkbox" id="b2" name="brand2" value="Eva Pharma">
                        <label for="b2">Eva Pharma</label>
                        <br>
                        <input type="checkbox" id="b3" name="brand3" value="GSK">
                        <label for="b3">GSK</label>
                        <br>
                        <input type="checkbox" id="b4" name="brand4" value="Eva Pharma">
                        <label for="b4">Eva Pharma</label>
                    </div>
            </div>

            <div class="product1">
                <div class="product-container1">
                    <div class="product-item">
                        <div class="product-img">
                            <img src="<%= %>" alt="<%= %>">
                            <div>
                                <button type="button" class="add-btnr">Add to Cart</button>
                            </div>
                        </div>
                        <div class="product-content">
                            <a href="./productDetails" title="<%= %>" class="product-title"> <%= %> </a>
                            <div>
                                <span class="price"><%= %></span>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <%- include ("./partials/footer.ejs") %>
</body>

</html>